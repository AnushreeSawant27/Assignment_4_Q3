   "source": [
    "# Write a Python program to square the elements of a list using map() function."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "887e767a",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Square of the list elements are: [16, 25, 4, 81]\n"
     ]
    }
   ],
   "source": [
    "def square_number(x):\n",
    "    return x ** 2\n",
    "\n",
    "sample_list = [4, 5, 2, 9]\n",
    "squared_numbers = list(map(square_number, sample_list))\n",
    "\n",
    "print('Square of the list elements are:',squared_numbers)"
   ]
  }
